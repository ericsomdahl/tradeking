{% extends "base.xml" %}
{% block order %}
  <Order>
    {% if price_type != 1 %} TmInForce="{{ time_in_force }}" {% endif %}
        Typ="{{ price_type }}" Side="{{ order_side }}" Acct="{{ account }}">
    <Instrmt SecTyp="{{ security_type }}" Sym="{{ symbol }}"/>
    <OrdQty Qty="{{ quantity }}"/>
  </Order>
{% endblock %}